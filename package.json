{
  "name": "generative.fm",
  "version": "1.33.0",
  "description": "A platform for playing generative music in the browser",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server",
    "start-production": "cross-env NODE_ENV=production webpack-dev-server --config webpack.prod.config.js",
    "build": "cross-env NODE_ENV=production webpack-cli --mode production --config webpack.prod.config.js",
    "predeploy": "npm run lint && env SAMPLE_FILE_HOST=//samples.alexbainter.com npm run build",
    "deploy:staging": "npm run predeploy && node node_scripts/deploy.js",
    "deploy:production": "npm run predeploy && node node_scripts/deploy.js -p",
    "lint": "eslint . --ext .js,.jsx",
    "postversion": "git push && git push --tags",
    "update-stats": "node node_scripts/update-stats.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metalex9/generative-music.git"
  },
  "author": "Alex Bainter <alexbainter+github@gmail.com> (http://github.com/metalex9)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/metalex9/generative-music/issues"
  },
  "homepage": "https://github.com/metalex9/generative-music#readme",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "app-manifest-loader": "^2.4.1",
    "aws-sdk": "^2.608.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.1",
    "eslint": "^4.19.1",
    "eslint-config-metalex9": "^0.1.1",
    "eslint-plugin-react": "^7.18.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "mime-types": "^2.1.26",
    "mini-css-extract-plugin": "^0.4.5",
    "node-gzip": "^1.1.2",
    "node-lame": "^1.2.0",
    "node-sass": "^4.13.1",
    "offline-plugin": "^5.0.7",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass-loader": "^7.3.1",
